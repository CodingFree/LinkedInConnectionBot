{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "/img/icons/imgg_19.png",
      "default_popup": "config_popup.html"
   },
   "content_scripts": [ {
      "js": [ "/js/external/jquery.js", "/js/src_changer.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Changes \u003Cimg> and \u003Ca> tags' link to it's host url",
   "icons": {
      "128": "/img/icons/img_128.png",
      "16": "/img/icons/img_16.png",
      "48": "/img/icons/img_48.png"
   },
   "manifest_version": 2,
   "name": "URL Changer",
   "version": "1.0"
}
